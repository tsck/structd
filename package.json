{
  "name": "dstruct",
  "version": "0.0.0",
  "description": "JavaScript data structure extension library",
  "main": "dist/dstruct.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "npm-run-all --parallel build:*",
    "build:umd": "webpack --output-filename dstruct.js",
    "build:umd.min": "webpack --output-filename dstruct.min.js -p",
    "commit": "git-cz",
    "check-coverage": "nyc check-coverage --statements 70 --branches 70 --functions 70 --lines 70 tap ./src",
    "watch:test": "npm t -- -w",
    "test": "mocha --recursive ./src --compilers js:babel-register",
    "cover": "nyc npm t"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tsck/dstruct.git"
  },
  "keywords": [
    "data",
    "structures",
    "set",
    "stack",
    "queue",
    "linked",
    "list",
    "dictionary",
    "hash",
    "table",
    "tree",
    "avl",
    "graph"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "author": "Terrence Keane <terrencekeane@gmail.com> (http://www.tkeane.me/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tsck/dstruct/issues"
  },
  "homepage": "https://github.com/tsck/dstruct#readme",
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-core": "6.21.0",
    "babel-loader": "6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-2": "^6.18.0",
    "babel-register": "6.18.0",
    "chai": "3.5.0",
    "commitizen": "^2.8.6",
    "cz-conventional-changelog": "^1.2.0",
    "ghooks": "^1.3.2",
    "istanbul": "^0.4.5",
    "mocha": "3.2.0",
    "npm-run-all": "3.1.2",
    "nyc": "10.0.0",
    "rimraf": "^2.5.4",
    "webpack": "1.14.0"
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run cover && npm run check-coverage"
    }
  },
  "babel": {
    "presets": [
      "stage-2",
      "es2015"
    ]
  }
}
