{
  "name": "structd",
  "description": "JavaScript data structure extension library",
  "version": "1.0.0-beta.1",
  "main": "dist/structd.js",
  "author": "Terrence Keane",
  "license": "MIT",
  "homepage": "https://github.com/tsck/structd#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tsck/structd.git"
  },
  "bugs": {
    "url": "https://github.com/tsck/structd/issues"
  },
  "keywords": [
    "data structures",
    "stack",
    "queue",
    "linked list",
    "dictionary",
    "tree",
    "graph"
  ],
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-core": "6.21.0",
    "babel-loader": "6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-2": "^6.18.0",
    "babel-register": "6.18.0",
    "chai": "3.5.0",
    "commitizen": "^2.8.6",
    "cz-conventional-changelog": "^1.2.0",
    "eslint": "3.12.2",
    "eslint-config-google": "0.7.1",
    "ghooks": "^1.3.2",
    "istanbul": "^0.4.5",
    "mocha": "3.2.0",
    "npm-run-all": "3.1.2",
    "nyc": "10.0.0",
    "rimraf": "^2.5.4",
    "webpack": "1.14.0"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "npm-run-all build:*",
    "build:umd": "webpack --output-filename structd.js",
    "build:umd.min": "webpack --output-filename structd.min.js -p",
    "commit": "git-cz",
    "check-coverage": "nyc check-coverage --statements 70 --branches 70 --functions 70 --lines 70 tap ./src",
    "lint": "eslint --ignore-pattern *.test.js src",
    "watch:test": "npm t -- -w",
    "test": "mocha --recursive ./src --compilers js:babel-register",
    "cover": "nyc npm t"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run cover && npm run check-coverage && npm run lint && npm run build"
    }
  },
  "babel": {
    "presets": [
      "stage-2",
      "es2015"
    ]
  }
}
